pension <- pension_inicial * annuity(i = i_12, n=12, type = "due")
i <- 1
t <- 1
max_iter <- 100
while((ahorro - pension) > 0 && t <= max_iter){
ahorro <- (ahorro - pension)* (1 + interes)
pension <- pension * (1+ crec_pensiones)
i <- i + 1
t <- t+ 1
}
if (t > max_iter) {
return(100)
} else {
return(i)
}
}
}
